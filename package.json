{
  "name": "globex-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "ng build --prod && ngc",
    "start": "node dist/server/server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run globex-ui:serve-ssr",
    "serve:ssr": "node dist/globex-ui/server/main.js",
    "prerender": "ng run globex-ui:prerender",
    "lint": "ng lint",
    "prod:docker": "ng build --configuration=production --output-hashing=none && ng run globex-ui:server:production --output-hashing=none",
    "dev:docker": "ng build --configuration=development  --output-hashing=none && ng run globex-ui:server:development --output-hashing=none"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.5",
    "@angular/common": "^15.1.5",
    "@angular/compiler": "^15.1.5",
    "@angular/core": "^15.1.5",
    "@angular/forms": "^15.1.5",
    "@angular/localize": "^15.1.5",
    "@angular/platform-browser": "^15.1.5",
    "@angular/platform-browser-dynamic": "^15.1.5",
    "@angular/platform-server": "^15.1.5",
    "@angular/router": "^15.1.5",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0",
    "@nguniversal/express-engine": "^14.2.3",
    "@popperjs/core": "^2.11.4",
    "axios": "^0.26.1",
    "body-parser": "^1.19.2",
    "bootstrap": "^5.1.3",
    "ci": "^2.1.1",
    "env-var": "^7.1.1",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "ngx-cookie-service": "^13.1.2",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "rxjs": "~7.5.0",
    "rxjs-compat": "^6.6.7",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.1.6",
    "@angular/compiler-cli": "^15.1.5",
    "@nguniversal/builders": "^14.2.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.8.4"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
